label subject  = "Subject Number"
      randomizedt = "Baseline visit date"
      month3dt    = "Month 3 visit date"
      month6dt    = "Month 6 visit date"
      painbase    = "Pain score at baseline: 0=none, 1=mild, 2=moderate, 3=severe"
